{
    "name": "ecodenl/woningdossier",
    "description": "Woningdossier",
    "keywords": [
        "woningdossier",
        "besparing",
        "woning",
        "econobis"
    ],
    "license": "OSL-3.0",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "darkaonline/l5-swagger": "^8.5",
        "doctrine/dbal": "^3.5",
        "ecodenl/ep-online-php-wrapper": "^5.0",
        "ecodenl/lvbag-php-wrapper": "^2.1.2",
        "guzzlehttp/guzzle": "^7.10",
        "laravel/fortify": "^1.31",
        "laravel/framework": "^12.36",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.30",
        "livewire/livewire": "^3.6.4",
        "maatwebsite/excel": "^3.1.67",
        "mpdf/mpdf": "^8.2",
        "owen-it/laravel-auditing": "^14.0",
        "plank/laravel-mediable": "^6.3",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^4.18",
        "spatie/laravel-permission": "^6.21",
        "spatie/laravel-translatable": "^6.11",
        "spatie/laravel-translation-loader": "^2.8",
        "symfony/http-client": "^7.2",
        "symfony/mailgun-mailer": "^7.2",
        "wedesignit/laravel-translations-import": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.1",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "squizlabs/php_codesniffer": "^3.13",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -WR",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Ecodenl/PicoWrapper"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
