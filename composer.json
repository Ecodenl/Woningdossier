{
    "name": "ecodenl/woningdossier",
    "description": "Woningdossier",
    "keywords": [
        "woningdossier",
        "besparing",
        "woning",
        "econobis"
    ],
    "license": "OSL-3.0",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "darkaonline/l5-swagger": "6.*",
        "doctrine/dbal": "^2.11",
        "ecodenl/pico-wrapper": "dev-master",
        "fideloper/proxy": "^4.4",
        "laravel/framework": "^6.20",
        "laravel/sanctum": "^2.9",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1.8",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.1",
        "spatie/laravel-permission": "^3.17",
        "spatie/laravel-translatable": "^4.6",
        "spatie/laravel-translation-loader": "^2.6",
        "wedesignit/laravel-translations-import": "^0.1.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "nunomaduro/collision": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "facade/ignition": "^1.16.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -WR"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Ecodenl/PicoWrapper"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
