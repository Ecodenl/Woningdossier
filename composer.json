{
    "name": "ecodenl/woningdossier",
    "description": "Woningdossier",
    "keywords": [
        "woningdossier",
        "besparing",
        "woning",
        "econobis"
    ],
    "license": "OSL-3.0",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "darkaonline/l5-swagger": "^8.0",
        "doctrine/dbal": "^3.5",
        "ecodenl/ep-online-php-wrapper": "^3.0",
        "ecodenl/lvbag-php-wrapper": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^10.48",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.11",
        "maatwebsite/excel": "^3.1.8",
        "mpdf/mpdf": "^8.1",
        "owen-it/laravel-auditing": "^13.1",
        "plank/laravel-mediable": "^5.9",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^3.2",
        "spatie/laravel-permission": "^5.8",
        "spatie/laravel-translatable": "^6.3",
        "spatie/laravel-translation-loader": "^2.7",
        "wedesignit/laravel-translations-import": "^1.1",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "larastan/larastan": "^2.4",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "*",
        "tinymce/tinymce": "^6.7",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -WR",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Ecodenl/PicoWrapper"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
